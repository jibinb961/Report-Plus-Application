<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MarkDisplayFragment">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ListView
            android:id="@+id/indiviudal_exam_marks_listview"
            android:layout_width="match_parent"
            android:layout_margin="10dp"
            android:background="@drawable/ellipse_border"
            android:layout_height="wrap_content"/>
        <Button
            android:id="@+id/cancel_button_marks_analytics"
            android:layout_width="match_parent"
            android:layout_margin="10dp"
            android:layout_height="wrap_content"
            android:text="Cancel"
            android:layout_below="@id/indiviudal_exam_marks_listview"
            android:textColor="@color/white"
            android:background="@drawable/roundbutton"/>
    </RelativeLayout>




</FrameLayout>